{
  "name": "libheif",
  "version": "1.19.5",
  "port-version": 3,
  "description": "libheif is an HEIF and AVIF file format decoder and encoder.",
  "homepage": "http://www.libheif.org/",
  "license": "LGPL-3.0-only",
  "supports": "!xbox",
  "dependencies": [
    {
      "name": "gdk-pixbuf",
      "platform": "!windows"
    },
    "libde265",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "hevc"
  ],
  "features": {
    "aom": {
      "description": "AVIF decoding and encoding via aom",
      "dependencies": [
        "aom"
      ]
    },
    "hevc": {
      "description": "HEVC encoding via x265",
      "dependencies": [
        "x265"
      ]
    },
    "iso23001-17": {
      "description": "Support for ISO23001-17 (uncompressed) codec"
    },
    "jpeg": {
      "description": "JPEG decoding and encoding via libjpeg-turbo",
      "dependencies": [
        "libjpeg-turbo"
      ]
    },
    "openjpeg": {
      "description": "JPEG-2000 decoding and encoding via OpenJPEG",
      "dependencies": [
        "openjpeg"
      ]
    }
  }
}